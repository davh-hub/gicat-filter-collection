{
	"packageName": "Basic Lean filter package",
	"authors": "David H., Frederic K.",
	"desc": "This package provides some basic filters for the Lean programming language.",
	"date": "2024-01-31T16:44:59.958Z",
	"nodeFilterList": [
		{
			"name": "Import Filter",
			"regex": "/import\\s+(\\S+)\\s*\\n*/gm",
			"id": "importFilter",
			"spec": "node",
			"exclude": [""],
			"extension": ".lean",
			"attributes": {
				"moduleName": [1]
			},
			"style": {
				"color": "#00B090"
			},
			"failures": [""],
			"label": "Import",
			"labelAttribute": "moduleName"
		},
		{
			"name": "Structure Filter",
			"regex": "/structure\\s+(\\S+)\\s+/gm",
			"id": "structureFilter",
			"spec": "node",
			"exclude": [""],
			"extension": ".lean",
			"attributes": {
				"structureName": [1]
			},
			"style": {
				"color": "#9E5EC4"
			},
			"failures": [""],
			"label": "Structure",
			"labelAttribute": "structureName"
		}
	],
	"edgeFilterList": [
	]
}